@* Indicates that the view expexts a list of Projects as its model *@
@model List<Project>

@{
    //razor block
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Projects</h2>

@if (Model.Any())
{
    //Iterate
    <table class="table">
        
        <thead>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        </thead>
        
        <tbody>
        @foreach (var project in Model)
        {
            <tr>
            <td>@project.ProjectId</td>
            <td>@project.Name</td>
            <td>@project.Description</td>
            <!-- Render any project properties as desired -->
            </tr>
        }   
        </tbody>
    </table>
}
else
{
    <p>No projects available</p>
}
